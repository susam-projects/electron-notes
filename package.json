{
  "name": "electron-notes",
  "productName": "electron-notes",
  "version": "1.0.1",
  "description": "It's a kind of offline blog",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts --ext .tsx .",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' electron-mocha --renderer --require ts-node/register --color src/renderer/**/*.test.ts",
    "pretty": "prettier --write src/**/*.{ts,tsx,css,scss}"
  },
  "keywords": [],
  "repository": "https://github.com/susam-projects/electron-diary.git",
  "author": "Susam",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./assets/app-icon.png"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_notes"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "icon": "./assets/app-icon.png"
            }
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Susam",
              "name": "electron-notes"
            },
            "prerelease": true
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@bbob/core": "^2.5.4",
    "@bbob/preset-react": "^2.5.4",
    "@bbob/react": "^2.5.4",
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@electron-forge/publisher-github": "^6.0.0-beta.50",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@openfonts/lora_cyrillic": "^1.44.0",
    "@openfonts/lora_latin": "^1.44.0",
    "@openfonts/open-sans_cyrillic": "^1.44.0",
    "@openfonts/open-sans_latin": "^1.44.0",
    "@types/chai": "^4.2.7",
    "@types/chai-arrays": "^1.0.3",
    "@types/classnames": "^2.2.9",
    "@types/codemirror": "^0.0.84",
    "@types/highlight.js": "^9.12.3",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-textarea-autosize": "^4.3.5",
    "@types/voca": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "autobind-decorator": "^2.4.0",
    "autoprefixer": "^9.7.3",
    "bootstrap": "3.4.1",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "chai-as-promised": "^7.1.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "codemirror": "^5.51.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "dexie": "^2.0.4",
    "electron": "8.2.0",
    "electron-mocha": "^8.1.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "highlight.js": "^9.18.1",
    "html-webpack-plugin": "^3.2.0",
    "iyobo-electron-devtools-installer": "^2.2.5",
    "lodash": "^4.17.15",
    "moment": "^2.29.2",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "remark-toc": "^7.0.0",
    "sass-loader": "^8.0.0",
    "sceditor": "^2.1.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "voca": "^1.4.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
